{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/authRoutes/$1"
        },
        {
            "src": "/api/product/(.*)",
            "dest": "/productRoutes/$1"
        },
        {
            "src": "/api/favorite/(.*)",
            "dest": "/favoriteRoutes/$1"
        },
        {
            "src": "/api/stripe/(.*)",
            "dest": "/checkoutRoutes/$1"
        },
        {
            "src": "/api(.*)",
            "dest": "/app.js"
        }
    ]
}