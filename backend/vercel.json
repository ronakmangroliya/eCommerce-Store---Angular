{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth(.*)",
            "dest": "/api/auth$1"
        },
        {
            "src": "/api/product(.*)",
            "dest": "/api/product$1"
        },
        {
            "src": "/api/favorite(.*)",
            "dest": "/api/favorite$1"
        },
        {
            "src": "/api/stripe(.*)",
            "dest": "/api/stripe$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/.*",
            "dest": "/app.js"
        }
    ]
}